<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:agac"
             x:Class="agac.App"
             RequestedThemeVariant="Light">
	<!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

	<Application.DataTemplates>
		<local:ViewLocator/>
	</Application.DataTemplates>

	<!-- Temas de la aplicacion -->
	
	<Application.Styles>
		<FluentTheme />
		<StyleInclude Source="Styles\ProgressRing.axaml"/>
	</Application.Styles>

	<!-- Recursos de la aplicacion -->
	
	<Application.Resources>
		<SolidColorBrush x:Key="NavBackground">#1f1f1f</SolidColorBrush>
		<SolidColorBrush x:Key="MenuBackground">#ebebeb</SolidColorBrush>
		<SolidColorBrush x:Key="HeaderMenu">#b5b5b5</SolidColorBrush>
		<SolidColorBrush x:Key="IconMenu">#949494</SolidColorBrush>
		<SolidColorBrush x:Key="BlueBackground">#1b396a</SolidColorBrush>
		
		<SolidColorBrush x:Key="White">#f5f5f5</SolidColorBrush>
		<SolidColorBrush x:Key="Green">#0d7a41</SolidColorBrush>
		<SolidColorBrush x:Key="Red">#e04141</SolidColorBrush>
		<SolidColorBrush x:Key="Blue">#205abd</SolidColorBrush>
		<SolidColorBrush x:Key="Black">#000000</SolidColorBrush>
		<SolidColorBrush x:Key="Gray">#737373</SolidColorBrush>
		<SolidColorBrush x:Key="GrayLight">#ebebeb</SolidColorBrush>
		<SolidColorBrush x:Key="GrayBorder">#d1d1d1</SolidColorBrush>

	</Application.Resources>
</Application>